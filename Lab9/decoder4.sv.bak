module decoder4#(parameter WIDTH = 32)
( 
	input logic [1:0] select,
	input logic[WIDTH:0] in,
	
	output logic[WIDTH:0] outA, outB, outC, outD
);

always_comb
begin
if(select == 2'b00)
	outA = in;
else if(select == 2'b01)
	outB = in;
else if(select == 2'b10)
	outC = in;
else //(select == 2'b11)
	outD = in;
end
endmodule
